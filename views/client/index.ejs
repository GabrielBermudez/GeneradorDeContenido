<!DOCTYPE html>
<html lang="en">
<head>
	<head><%- include('../layouts/head');%></head>
</head>
<body>
	<header><%- include('../layouts/header');%></header>
		<div class="container">
			<div class="row">
				<% empresas.forEach((empresa) => { %>
					<div class="col-md-6 m-5" style="width: 400px">
						<a href="/client/home/id/<%=empresa.id_empresa%>">
							<div class="card">
							 	<img src="/images/producto.jpeg" class="card-img-top" alt="...">
							  	<div class="card-body d-flex justify-content-center">
							    	<%=empresa.denominacion%>
							  	</div>
							</div>
						</a>
					</div>	
				<%})%>
			</div>
		</div>
		<div class="d-flex justify-content-center mb-3 mt-3">
        <button type="button" class="btn btn-warning" onclick="window.history.go(-1)"><i class="fa fa-reply"></i>
        Volver</button>
    </div>
	
	<footer><%- include('../layouts/footer');%></footer>
</body>
</html>